@model xdcb.HoSoCongTrinhs.ComponentsView.IndexModel

<abp-modal centered="true" size="Large" id="modalNoiDungYeuCauChinhSua">
    <div class="modal-header">
        <h5 class="modal-title">Nội dung Yêu cầu chỉnh sửa</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <abp-modal-body>
        <div class="container-fluid">
            <div class="row">
                <div class="col-12" id="noiDungYeuCauChinhSua">
                </div>
            </div>
        </div>
    </abp-modal-body>
    <abp-modal-footer>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
    </abp-modal-footer>
</abp-modal>
